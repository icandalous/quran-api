   "sourate": "مريم",
   "sourate_number": 19,
   "ayat_number": "22"
 },
 {
   "ayat_content": "فَرَجَعْنَاكَ إِلَى أُمِّكَ كَيْ تَقَرَّ عَيْنُهَا وَلا تَحْزَنَ",
