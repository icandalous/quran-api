   "sourate_number": 20,
   "ayat_number": "40"
 },
 {
   "ayat_content": "فَرَدَدْنَاهُ إِلَى أُمِّهِ كَيْ تَقَرَّ عَيْنُهَا وَلا تَحْزَنَ",
