   "sourate_number": 2,
   "ayat_number": "60"
 },
 {
   "ayat_content": "فَانْبَجَسَتْ مِنْهُ اثْنَتَا عَشْرَةَ عَيْناً}",
   "sourate": "الأعراف",
   "sourate_number": 7,
   "ayat_number": "160"
