   "sourate": "المؤمنون",
   "sourate_number": 23,
   "ayat_number": "33"
 },
 {
   "ayat_content": "قَدْ وُعِدْنَا نَحْنُ وَآبَاؤُنَا هَذَا مِنْ قَبْلُ إِنْ هَذَا إِلاَّ أَسَاطِيرُ الأَوَّلِينَ",
   "sourate": "المؤمنون",
   "sourate_number": 23,
   "ayat_number": "83"
 },
 {
   "ayat_content": "لَقَدْ وُعِدْنَا هَذَا نَحْنُ وَآبَاؤُنَا مِنْ قَبْلُ إِنْ هَذَا إِلاَّ أَسَاطِيرُ الأَوَّلِينَ",
   "sourate": "النمل",
   "sourate_number": 27,
   "ayat_number": "68"
 },
 {
   "ayat_content": "وَلَوْلا فَضْلُ اللَّهِ عَلَيْكُمْ وَرَحْمَتُهُ وَأَنَّ اللَّهَ تَوَّابٌ حَكِيمٌ",
   "sourate": "النور",
   "sourate_number": 24,
   "ayat_number": "10"
 },
 {
   "ayat_content": "وَلَوْلا فَضْلُ اللَّهِ عَلَيْكُمْ وَرَحْمَتُهُ وَأَنَّ اللَّهَ رَؤُوفٌ رَحِيمٌ",
   "sourate": "النور",
   "sourate_number": 24,
   "ayat_number": "20"
