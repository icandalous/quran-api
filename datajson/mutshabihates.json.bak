   "sourate_number": 28,
   "ayat_number": "13"
 },
 {
   "ayat_content": "مَا يَأْتِيهِمْ مِنْ ذِكْرٍ مِنْ رَبِّهِمْ مُحْدَثٍ إِلاَّ اسْتَمَعُوهُ وَهُمْ يَلْعَبُونَ",
   "sourate": "الأنبياء",
   "sourate_number": 21,
   "ayat_number": "2"
