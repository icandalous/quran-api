 {
   "ayat_content": "فَكُلُوا مِنْهَا وَأَطْعِمُوا الْقَانِعَ وَالْمُعْتَرَّ",
   "sourate": "الحج",
   "sourate_number": 22,
   "ayat_number": "36"
 },
 {
   "ayat_content": "فَقَالَ الْمَلأُ الَّذِينَ كَفَرُوا مِنْ قَوْمِهِ مَا هَذَا إِلاَّ بَشَرٌ مِثْلُكُمْ",
